<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <custom-type type-id="NewsletterSubscription">
        <display-name xml:lang="x-default">Newsletter Training</display-name>
        <staging-mode>no-staging</staging-mode>
        <storage-scope>site</storage-scope>
        <key-definition attribute-id="email">
            <type>string</type>
            <min-length>0</min-length>
        </key-definition>
        <attribute-definitions>
            <attribute-definition attribute-id="firstName">
                <display-name xml:lang="x-default">First Name</display-name>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="lastName">
                <display-name xml:lang="x-default">Last Name</display-name>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </attribute-definitions>
        <group-definitions>
            <attribute-group group-id="newsletter_subscription_group">
                <display-name xml:lang="x-default">Newsletter Subscription Group</display-name>
                <attribute attribute-id="firstName"/>
                <attribute attribute-id="lastName"/>
                <attribute attribute-id="email"/>
            </attribute-group>
        </group-definitions>
    </custom-type>

    <custom-type type-id="custom_object_training">
        <display-name xml:lang="x-default">Custom Object Training</display-name>
        <staging-mode>no-staging</staging-mode>
        <storage-scope>organization</storage-scope>
        <key-definition attribute-id="customObjectTraining">
            <type>string</type>
            <min-length>0</min-length>
        </key-definition>
        <group-definitions>
            <attribute-group group-id="custom_object_training_attr_group">
                <display-name xml:lang="x-default">Custom Object Attribute Group</display-name>
                <attribute attribute-id="customObjectTraining"/>
                <attribute attribute-id="creationDate" system="true"/>
                <attribute attribute-id="lastModified" system="true"/>
            </attribute-group>
        </group-definitions>
    </custom-type>

</metadata>
